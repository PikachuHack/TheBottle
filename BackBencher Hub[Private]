local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Grape Hub", "GrapeTheme")

--Tabs
local Tab1 = Window:NewTab("Buy Items")
local Tab2 = Window:NewTab("Misc")
local Tab3 = Window:NewTab("drop items")
local Tab4 = Window:NewTab("Credits")

--Sections inside tabs
local Tab1Section = Tab1:NewSection("Buy Items")
