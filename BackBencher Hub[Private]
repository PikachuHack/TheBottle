local Tab4Section = Tab4:NewSection("Thank you for using this script!")

--Buttons
Tab1Section:NewButton("Buy F", "Buys F", function()
  game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer("F")
end)
Tab1Section:NewToggle("auto buy f", "auto buys f", function(state)
    if state then
        _G.on = true
        while _G.on do
            wait()
            local args = {
                [1] = "F"
            }
            
            game:GetService("ReplicatedStorage").RemoteEvents.BuyItemCash:FireServer(unpack(args))
        end
    else
        _G.on = false
    end
end)
